<?xml version="1.0" encoding="ISO-8859-1"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="[IAL] Fluxo de autos recebidos da autuação">
    <description><![CDATA[cd_fluxo: IAL_FLX_AUT_REC
Descrição: Fluxo destinado ao autos recém protocolados.]]></description>  
    <!-- SWIMLANES -->
    <swimlane name="Nó de Desvio - [IAL] Fluxo de autos recebidos da autuação">
        <assignment pooled-actors="#{localizacaoAssignment.getPooledActors('10:1338,9:1469')}"/>
    </swimlane>
    <swimlane name="solicitante">
        <assignment actor-id="#{actor.id}"/>
    </swimlane>
    <swimlane name="Secretaria e Gabinete">
        <assignment pooled-actors="#{localizacaoAssignment.getPooledActors('10:1338')}"/>
    </swimlane>  
    <!-- START-STATE -->
    <start-state name="Início">
        <task name="Tarefa inicial" swimlane="solicitante" priority="3"/>
        <transition to="[IAL] Autos recebidos - Analisar" name="[IAL] Autos recebidos - Analisar"/>
    </start-state>  
    <!-- NODES -->
    <task-node end-tasks="true" name="[IAL] Autos recebidos - Analisar">
        <task name="[IAL] Autos recebidos - Analisar" swimlane="Secretaria e Gabinete" priority="3"/>
        <transition to="Nó de Desvio - [IAL] Fluxo de autos recebidos da autuação" name="Nó de Desvio - [IAL] Fluxo de autos recebidos da autuação">
            <condition expression="#{true}"/>
        </transition>
        <transition to="[IAL] Encaminhar para o fluxo de audiência" name="[IAL] Encaminhar para o fluxo de audiência"/>
    </task-node>
    <node name="[IAL] Encaminhar para o fluxo de audiência">
        <transition to="Término" name="Término"/>
        <event type="node-enter">
            <action expression="#{processoJudicialService.incluirNovoFluxo(tramitacaoProcessualService.recuperaProcesso(), 'IAL_FLX_OP_AUD')}"/>
        </event>
    </node>
    <end-state name="Término"/>
    <task-node end-tasks="true" name="Nó de Desvio - [IAL] Fluxo de autos recebidos da autuação">
        <task name="Nó de Desvio - [IAL] Fluxo de autos recebidos da autuação" swimlane="Nó de Desvio - [IAL] Fluxo de autos recebidos da autuação" priority="3"/>
        <transition to="Término" name="Término"/>
        <transition to="[IAL] Autos recebidos - Analisar" name="[IAL] Autos recebidos - Analisar"/>
    </task-node>  
    <!-- PROCESS-EVENTS -->
    <event type="superstate-enter">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="process-start">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="before-signal">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-create">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="subprocess-created">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
